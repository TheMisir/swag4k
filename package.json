{
  "name": "swag4k",
  "version": "1.0.4",
  "description": "A swagger endpoint creater for koa apps",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha test/test-*.js",
    "jsdoc": "jsdoc -c jsdoc.json",
    "validate": "npm run coverage && istanbul check-coverage --statement 75 --branch 75 --function 75 --line 75",
    "build": "npm i && npm run jsdoc && npm run validate",
    "clean": "rm -rf node_modules"
  },
  "keywords": [
    "swagger",
    "koa",
    "swagger-ui",
    "swagger for koa",
    "jsdoc",
    "api",
    "joi",
    "joi to swagger"
  ],
  "author": "Mike Brennan",
  "contributors": [],
  "dependencies": {
    "doctrine": "2.0.0",
    "joi-to-swagger": "^1.1.0",
    "koa-router": "5.2.3",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "co": "^4.6.0",
    "co-mocha": "^1.1.3",
    "decache": "^4.1.0",
    "joi": "10.2.1",
    "koa": "1.1.2",
    "mocha": "^3.2.0",
    "lodash": "4.6.0",
    "mock-require": "^2.0.0",
    "request": "^2.79.0",
    "sinon": "^1.17.6",
    "supertest": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "url here when in Stash"
  },
  "private": false,
  "license": "UNLICENSED",
  "directories": {
    "test": "test"
  }
}
